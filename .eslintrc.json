{
    "extends": "airbnb",
  
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".ios.js",
            ".android.js"
          ]
        }
      }
    },
    "rules": {
        "no-use-before-define":0,
        "react/jsx-filename-extension":0}
        
    "env": {
      "node": true
    }
}